---
import { SEO } from "astro-seo";
import type { ComponentProps } from "astro/types";

type Props = Omit<ComponentProps<typeof SEO>, "extend">;
---

<head>
  <SEO
    charset="UTF-8"
    extend={{
      meta: [
        {
          name: "viewport",
          content: "width=device-width",
        },
      ],
      link: [{ rel: "icon", type: "image/svg+xml", href: "/favicon.svg" }],
    }}
    {...Astro.props}
  />
</head>
